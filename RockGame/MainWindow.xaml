<Window x:Class="RockGame.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RockGame"
        mc:Ignorable="d"
        Title="MainWindow" Height="569" Width="511">
    <Grid Margin="0,0,51,66">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="236*"/>
            <ColumnDefinition Width="269*"/>
        </Grid.ColumnDefinitions>
        <Button x:Name="btnOpenMap" Content="Карта" HorizontalAlignment="Left" Margin="58,10,0,0" VerticalAlignment="Top" Height="19" Width="81" Click="btnOpenMap_Click"/>
        <Canvas x:Name="game" HorizontalAlignment="Left" Height="510" Margin="0,40,0,0" VerticalAlignment="Top" Width="507" Grid.ColumnSpan="2"/>
    </Grid>
</Window>
